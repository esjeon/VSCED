{
  "manifest_version": 2,

  "name": "VSCED",
  "description": "VSCode Extension Downloader",
  "version": "0.1",

  "permissions" : [
      "downloads"
  ],

  "background": {
      "scripts": ["background.js"]
  },

  "content_scripts": [
      {
          "matches": ["https://marketplace.visualstudio.com/items?*"],
          "js": ["content-item.js"]
      }
  ]
}